#!/bin/sh

echo "starting /etc/user-service"
services=$(ls /etc/user-service)

for service in $services
do
    mkdir "/etc/user-service/${service}/supervise"
done

chown -R user-service:user-service /etc/user-service
exec /sbin/chpst -e /etc/envvars -u user-service:user-service /sbin/runsvdir /etc/user-service